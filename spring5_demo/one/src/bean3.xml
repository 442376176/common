<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd"
>
<bean id="stu" class="com.zcc.demo.bean.Student">
    <property name="courses">
        <array>
            <value>java</value>
            <value>sql</value>
        </array>
    </property>
    <property name="list">
        <list>
            <value>a</value>
            <value>b</value>
            <value>c</value>
            <value>d</value>
        </list>
    </property>
    <property name="map">
        <map>
            <entry key="a">
                <value>a</value>
            </entry>
            <entry key="b" value="b"/>
        </map>
    </property>
    <property name="set">
        <set>
            <value>1</value>
            <value>1</value>
            <value>1</value>
            <value>3</value>
            <value>2</value>

        </set>
    </property>

</bean>
</beans>